<h2>Register</h2>

<form action="new" method="post" enctype="multipart/form-data" onsubmit="return validateFile();">
	<h3>Name</h3>
	<input type="text" name="user[name]">
	<h3>Email</h3>
	<input type="text" name="user[email]">
	<h3>Password</h3>
	<input type="password" name="user[password]">
	<h3>Avatar</h3>
	<input id="imageUpload" type="file" name="file">
	<h3>About me</h3>
	<textarea name="user[about]" rows="10" cols="50"></textarea>
	<br>
	<input type="submit" name="submit">
</form>
